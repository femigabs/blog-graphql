{
  "preset": "ts-jest",
  "testMatch": ["<rootDir>/test/**/*.{js,ts}"],
  "testEnvironment": "node","coverageDirectory": "coverage",
  "collectCoverageFrom": ["app/**/*.{js,ts}"],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "transform": {
    "^.+\\.ts?$": "ts-jest"
  },
  "transformIgnorePatterns": ["<rootDir>/node_modules/"],
  "setupFilesAfterEnv": ["<rootDir>/test/setup.ts"]
}
